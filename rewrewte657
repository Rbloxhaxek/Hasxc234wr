while true do
    wait()
    local plrteam = game.Players.LocalPlayer.Team.Name
    if plrteam == "Prisoner" or "Police" or "Criminal" then
        require(game:GetService("ReplicatedStorage").Game.Notification).new({Text = "Script is currently bugged, i will let everyone know when it is fixed", Duration = 5})
